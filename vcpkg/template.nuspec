<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ros-dashing-rqt-image-view</id>
    <version>1.0.2</version>
    <title>ros-dashing-rqt-image-view</title>
    <authors>Dirk Thomas</authors>
    <owners>OSRF</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>rqt_image_view provides a GUI plugin for displaying images using image_transport.</description>
    <dependencies>
		<dependency id="ros-dashing-cv-bridge"/>
		<dependency id="ros-dashing-geometry-msgs"/>
		<dependency id="ros-dashing-image-transport"/>
		<dependency id="ros-dashing-qt-gui-cpp"/>
		<dependency id="ros-dashing-rclcpp"/>
		<dependency id="ros-dashing-rqt-gui"/>
		<dependency id="ros-dashing-rqt-gui-cpp"/>
		<dependency id="ros-dashing-sensor-msgs"/>
		<dependency id="ros-dashing-ros-workspace"/>

    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
